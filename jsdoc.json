{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["client/js", "server", "shared"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "./doc/",
        "recursive": true
    }
}
